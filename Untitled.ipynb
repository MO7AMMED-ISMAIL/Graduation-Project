   "source": [
    "import serial\n",
    "import time\n",
    "\n",
    "ser = serial.Serial('COM3', 9600)\n",
    "time.sleep(2)  # wait for the Arduino to initialize\n",
    "\n",
    "ser.write(b'1')  # turn on the LED\n",
    "time.sleep(1000)\n",
    "\n",
    "ser.write(b'0')  # turn off the LED\n",
    "time.sleep(1000)\n",
    "\n",
    "ser.close()"
   ]
